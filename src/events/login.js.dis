const Event = require('../library/Structures/Event');
const repl = require('repl');

class Login extends Event {

	run() {
		const repl2 = repl.start(`${this.client.bot.username} > `);
		repl2.context.client = this.client;

		repl2.on('exit', () => {
			this.client.bot.end();
		});
	}

}

module.exports = Login;
